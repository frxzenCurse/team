mixin header 
  header.header 
    .header__inner
      .header__row 
        .header__col
          .header__logo
            a.header__logo-link(href="#")
              +logo-svg.header__svg 
          .header__phone
            a.header__phone-link(href="tel:8 958 581-50-84") 
              |8 958 581-50-84
          nav.header__nav 
            .header__nav-wrapper
              .header__nav-button(data-header-button='')
                |Меню
            .header__nav-container
              ul.header__list(data-header-accordion='')
                each item in ['О нас', 'Компетенции', 'Стоимость', 'Команда', 'FAQ', 'Процесс', 'Отзывы и клиенты',]
                  li.header__item 
                    a.header__link(href="#")
                      |#{item}
        .header__col
          .header__button(data-fancy-button='discuss')
            |Обсудить проект
          .header__menu(data-fancy-button='menu')
            span.header__menu-span
            span.header__menu-span
      ul.header__template(data-header-accordion='')
        each item in ['О нас', 'Компетенции', 'Стоимость', 'Команда', 'FAQ', 'Процесс', 'Отзывы и клиенты',]
          li.header__item 
            a.header__link(href="#")
              |#{item}